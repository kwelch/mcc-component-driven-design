// Syntactically Awesome StyleSheets (SASS)

.global-header {
  background: hsl(202, 70%, 90%);
  color: hsl(202, 0%, 100%);
  height: 40px;
  padding: 10px;
  &-logo {
    float: left;
    &-image {
      background: url("logo.png");
      height: 40px;
      width: 200px;
    }
  }
  &-nav {
    float: right;
  }
}